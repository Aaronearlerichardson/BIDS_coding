{"dataFormat" : [".nii", ".nii.gz", ".gz", ".edf", ".mgz", ".txt", ".mat"]
,"eventFormat" : ["Subject", "Trial", "Rec", "Day", "FilenamePrefix", "Start",
    "Auditory", "Go", "StartCode", "AuditoryCode", "GoCode", "Noisy", "NoResponse",
    "ResponseStart","ResponseEnd","cue", "sound", "go", "cueTime", "block", "cueStart",
     "Trigger", "cueEnd", "audioStart", "audioAlignedTrigger", "delStart", "delEnd",
      "goStart", "goEnd", "respStart", "respEnd", "isiStart", "isiEnd", "tCaptureStart"]
,"eventFormat.IDcol" : "FilenamePrefix"
, "compress" : true
, "repetitionTimeInSec" : 2
, "delayTimeInSec" : [ 0.028, 0.013, 0.026, 0.040]
, "series" : "odd-interleaved"
, "anat": {"left" : ".*?"
    , "right" : ".*?"
    , "content" : [ ["T1w","T1"]
                    ,["CT","CT"]
]
    }   
, "ieeg": {"left" : ".*?"
            , "right" : ".*?"
            , "content" : [   ["ieeg",".edf"]
                            ]
            }
, "ieeg.task": {"left" : "_"
            , "right" : ".*?"
            , "content" : [ ["Phoneme_Sequence","PHONEMESEQUENCE"]
                            ,["Phoneme_Sequence", "PhonemeSequence"]
                            ,["Phoneme_Sequence", "PhonemeSequencing"]
]
    }
, "ieeg.loc":{"left" : ".*?"
    , "right" : ".*?"
    , "content" : [   ["RAS",".txt"]
                    ]
    }
            
, "sessLabel" : {"left" : "^[0-9]*?"
                , "right" : "\\.[(edf)(mat)]"
                , "content" :   ["[0-9]"]
                }
, "runIndex" : {"left" : "run"
                , "right" : "_"
                , "content" : ["[0-9]{2}"]
                }
, "partLabel" : {"left" : ".*?"
                , "right" : "[_ ]"
                , "content" : ["D[0-9]{1,4}"]
                }
, "echo" : {"left" : "e"
    , "right" : "\\."
    , "content" : ["[0-9]{2}"]
    }
}
